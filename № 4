user_list = input("Введите ряд 10 чисел ").split()
m = min(user_list)
ma = max(user_list)
print(m, ma,)








while True:
    user_list = input("Введите ряд 20 чисел ").split()
    m = min(user_list)
    ma = max(user_list)
    print(m, ma)
    user_answer = input("Хотите сделать еще раз? (да/нет)")
    if user_answer == 'нет':
        break
